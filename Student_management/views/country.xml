<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="Country_tree" model="ir.ui.view">
        <field name="name">Country_tree</field>
        <field name="model">student.country</field>
        <field name="arch" type="xml">
                <tree>
                    <field name="student_name_ids"/>
                    <field name="name"/>
                    <field name="country_code"/>

                </tree>
        </field>
    </record>

    <record id="Country_form" model="ir.ui.view">
        <field name="name">Country_form</field>
        <field name="model">student.country</field>
        <field name="arch" type="xml">
                <form string = "">
                    <group string = "">
                        <field name="student_name_ids"/>
                        <field name="name"/>
                        <field name="country_code"/>
                    </group>
                </form>
        </field>
    </record>
</odoo>
